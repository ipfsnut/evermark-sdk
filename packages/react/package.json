{
  "name": "@ipfsnut/evermark-sdk-react",
  "version": "1.0.1",
  "description": "React components and hooks for image loading with storage orchestration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test:components": "vitest --run components.test.tsx",
    "test:hooks": "vitest --run hooks.test.ts"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@ipfsnut/evermark-sdk-core": "workspace:*",
    "@ipfsnut/evermark-sdk-storage": "workspace:*",
    "@ipfsnut/evermark-sdk-browser": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.3.0",
    "@vitest/coverage-v8": "^1.3.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2"
  },
  "keywords": [
    "react",
    "image",
    "components",
    "hooks",
    "storage",
    "upload",
    "ui",
    "evermark",
    "blockchain",
    "web3"
  ],
  "author": "ipfsnut",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfsnut/evermark-sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/ipfsnut/evermark-sdk/issues"
  },
  "homepage": "https://github.com/ipfsnut/evermark-sdk#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}